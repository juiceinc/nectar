<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
  <application>sliceboard</application>
  <version>2</version>
  <system-properties>
    <!-- Set-up the app engine JDK logging. -->
    <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
    <!-- Configure Scalate to use only precompiled templates. -->
    <property name="scalate.allowReload" value="false"/>
    <property name="scalate.workdir" value="WEB-INF"/>
  </system-properties>
  <sessions-enabled>true</sessions-enabled>
  <static-files>
    <include path="/config/**"/>
    <include path="/images/**"/>
    <include path="/js/**"/>
  </static-files>
</appengine-web-app>
